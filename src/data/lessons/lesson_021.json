  "dialogue": {
    "lines": [
      {
        "text": "Geef me een boek, alsjeblieft, Jane.",
        "speaker": "Speaker 1",
        "translation": "Dame un libro, por favor, Jane."
      },
      {
        "text": "Welk boek?",
        "speaker": "Speaker 2",
        "translation": "¿Qué libro?"
      },
      {
        "text": "Deze?",
        "speaker": "Speaker 1",
        "translation": "¿Éste?"
      },
      {
        "text": "Nee, niet die. De rode.",
        "speaker": "Speaker 2",
        "translation": "No, no ese. El rojo."
      },
      {
        "text": "Deze?",
        "speaker": "Speaker 1",
        "translation": "¿Éste?"
      },
      {
        "text": "Ja, alsjeblieft.",
        "speaker": "Speaker 2",
        "translation": "Sí, por favor."
      },
      {
        "text": "Alsjeblieft.",
        "speaker": "Speaker 1",
        "translation": "Aquí tienes."
      },
      {
        "text": "Dank je.",
        "speaker": "Speaker 2",
        "translation": "Gracias."
      }
    ],
