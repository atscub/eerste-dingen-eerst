  "sections": [
    {
      "type": "dialogue",
      "lines": [
        {
          "text": "Geef me een boek, alsjeblieft, Jane.",
          "speaker": "Man",
          "translation": "Dame un libro, por favor, Jane."
        },
        {
          "text": "Welk boek?",
          "speaker": "Jane",
          "translation": "¿Qué libro?"
        },
        {
          "text": "Deze?",
          "speaker": "Man",
          "translation": "¿Éste?"
        },
        {
          "text": "Nee, niet die. De rode.",
          "speaker": "Jane",
          "translation": "No, no ese. El rojo."
        },
        {
          "text": "Deze?",
          "speaker": "Man",
          "translation": "¿Éste?"
        },
        {
          "text": "Ja, alsjeblieft.",
          "speaker": "Jane",
          "translation": "Sí, por favor."
        },
        {
          "text": "Alsjeblieft.",
          "speaker": "Man",
          "translation": "Aquí tienes."
        },
        {
          "text": "Dank je.",
          "speaker": "Jane",
          "translation": "Gracias."
        }
      ],
