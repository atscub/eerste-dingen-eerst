  "title": "Les 41",
  "sections": [
    {
      "type": "dialogue",
      "lines": [
        {
          "text": "Is die tas zwaar, Mary?",
          "speaker": "Mary",
          "translation": "¿Es esa bolsa pesada, Mary?"
        },
        {
          "text": "Niet erg zwaar.",
          "speaker": "Sam",
          "translation": "No muy pesada."
        },
        {
          "text": "Hier! Zet hem op deze stoel. Wat zit erin?",
          "speaker": "Mary",
          "translation": "¡Aquí! Ponla sobre esta silla. ¿Qué hay en ella?"
        },
        {
          "text": "Een stuk kaas.",
          "speaker": "Sam",
          "translation": "Un pedazo de queso."
        },
        {
          "text": "Is dat blik tabak voor mij?",
          "speaker": "Mary",
          "translation": "¿Es esa lata de tabaco para mí?"
        },
        {
          "text": "Nou, het is zeker niet voor mij!",
          "speaker": "Sam",
          "translation": "Bueno, ¡ciertamente no es para mí!"
        }
      ],
