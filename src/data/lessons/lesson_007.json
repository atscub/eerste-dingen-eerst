      "type": "dialogue",
      "lines": [
        {
          "text": "Ik ben een nieuwe student. Mijn naam is Robert.",
          "speaker": "Robert",
          "translation": "Soy un estudiante nuevo. Mi nombre es Robert."
        },
        {
          "text": "Hoe maakt u het? Mijn naam is Alice.",
          "speaker": "Alice",
          "translation": "¿Cómo está usted? Mi nombre es Alice."
        },
        {
          "text": "Bent u Frans?",
          "speaker": "Robert",
          "translation": "¿Es usted francesa?"
        },
        {
          "text": "Ja, dat ben ik.",
          "speaker": "Alice",
          "translation": "Sí, lo soy."
        },
        {
          "text": "Bent u ook Frans?",
          "speaker": "Robert",
          "translation": "¿Es usted francés, también?"
        },
        {
          "text": "Nee, dat ben ik niet.",
          "speaker": "Alice",
          "translation": "No, no lo soy."
        },
        {
          "text": "Wat is uw nationaliteit?",
          "speaker": "Robert",
          "translation": "¿Cuál es su nacionalidad?"
        },
        {
          "text": "Ik ben Italiaans.",
          "speaker": "Alice",
          "translation": "Soy italiano."
        },
        {
          "text": "Bent u lerares?",
          "speaker": "Robert",
          "translation": "¿Es usted profesora?"
        },
        {
          "text": "Nee, dat ben ik niet.",
          "speaker": "Alice",
          "translation": "No, no lo soy."
        },
        {
          "text": "Wat is uw beroep?",
          "speaker": "Robert",
          "translation": "¿Cuál es su profesión?"
        },
        {
          "text": "Ik ben typiste.",
          "speaker": "Alice",
          "translation": "Soy mecanógrafa."
        },
        {
          "text": "Wat is uw beroep?",
          "speaker": "Robert",
          "translation": "¿Cuál es su profesión?"
        },
        {
          "text": "Ik ben ingenieur.",
          "speaker": "Alice",
          "translation": "Soy ingeniero."
        }
      ],
