  "dialogue": {
    "lines": [
      {
        "text": "Geef me wat glazen, alsjeblieft, Jane.",
        "speaker": "Speaker 1",
        "translation": "Dame unos vasos, por favor, Jane."
      },
      {
        "text": "Welke glazen?",
        "speaker": "Speaker 2",
        "translation": "¿Qué vasos?"
      },
      {
        "text": "Deze?",
        "speaker": "Speaker 1",
        "translation": "¿Éstos?"
      },
      {
        "text": "Nee, niet die. Die op de plank.",
        "speaker": "Speaker 2",
        "translation": "No, no ésos. Los que están en el estante."
      },
      {
        "text": "Deze?",
        "speaker": "Speaker 1",
        "translation": "¿Éstos?"
      },
      {
        "text": "Ja, alsjeblieft.",
        "speaker": "Speaker 2",
        "translation": "Sí, por favor."
      },
      {
        "text": "Alsjeblieft.",
        "speaker": "Speaker 1",
        "translation": "Aquí tienes."
      },
      {
        "text": "Dank je.",
        "speaker": "Speaker 2",
        "translation": "Gracias."
      }
    ],
