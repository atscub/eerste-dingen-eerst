  "type": "dialogue",
  "dialogue": {
    "lines": [
      {
        "text": "Van wie is dit overhemd?",
        "speaker": "Leraar",
        "translation": "¿De quién es esta camisa?"
      },
      {
        "text": "Is dit jouw overhemd, Frank?",
        "speaker": "Leraar",
        "translation": "¿Es ésta tu camisa, Frank?"
      },
      {
        "text": "Nee, meneer. Het is niet mijn overhemd.",
        "speaker": "Frank",
        "translation": "No, señor. No es mi camisa."
      },
      {
        "text": "Dit is mijn overhemd. Mijn overhemd is blauw.",
        "speaker": "Frank",
        "translation": "Ésta es mi camisa. Mi camisa es azul."
      },
      {
        "text": "Is dit het overhemd van Tim?",
        "speaker": "Leraar",
        "translation": "¿Es ésta la camisa de Tim?"
      },
      {
        "text": "Misschien, meneer. Het overhemd van Tim is wit.",
        "speaker": "Frank",
        "translation": "Quizás lo sea, señor. La camisa de Tim es blanca."
      },
      {
        "text": "Tim!",
        "speaker": "Leraar",
        "translation": "¡Tim!"
      },
      {
        "text": "Ja, meneer?",
        "speaker": "Tim",
        "translation": "¿Sí, señor?"
      },
      {
        "text": "Is dit jouw overhemd?",
        "speaker": "Leraar",
        "translation": "¿Es ésta tu camisa?"
      },
      {
        "text": "Ja, meneer.",
        "speaker": "Tim",
        "translation": "Sí, señor."
      },
      {
        "text": "Alsjeblieft. Vang!",
        "speaker": "Leraar",
        "translation": "Aquí tienes. ¡Agárrala!"
      },
      {
        "text": "Dank u, meneer.",
        "speaker": "Tim",
        "translation": "Gracias, señor."
      }
    ],
