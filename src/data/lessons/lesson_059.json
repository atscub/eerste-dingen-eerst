  "type": "dialogue",
  "dialogue": {
    "lines": [
      {
        "text": "Ik wil wat enveloppen, alstublieft.",
        "speaker": "Speaker 1",
        "translation": "Quiero unos sobres, por favor."
      },
      {
        "text": "Wilt u de grote maat, of de kleine maat?",
        "speaker": "Speaker 2",
        "translation": "¿Quiere el tamaño grande, o el tamaño pequeño?"
      },
      {
        "text": "De grote maat, alstublieft.",
        "speaker": "Speaker 1",
        "translation": "El tamaño grande, por favor."
      },
      {
        "text": "Heeft u schrijfpapier?",
        "speaker": "Speaker 2",
        "translation": "¿Tiene papel de escribir?"
      },
      {
        "text": "Ja, dat hebben we.",
        "speaker": "Speaker 1",
        "translation": "Sí, tenemos."
      },
      {
        "text": "Ik heb geen kleine blokken. Ik heb alleen grote. Wilt u een blok?",
        "speaker": "Speaker 2",
        "translation": "No tengo almohadillas pequeñas. Solo tengo grandes. ¿Quiere una almohadilla?"
      },
      {
        "text": "Ja, alstublieft.",
        "speaker": "Speaker 1",
        "translation": "Sí, por favor."
      },
      {
        "text": "En ik wil wat zwarte inkt en wat lijm.",
        "speaker": "Speaker 2",
        "translation": "Y quiero algo de tinta negra y algo de pegamento."
      },
      {
        "text": "Een fles inkt en een fles lijm.",
        "speaker": "Speaker 1",
        "translation": "Una botella de tinta y una botella de pegamento."
      },
      {
        "text": "En ik wil ook een grote doos krijt.",
        "speaker": "Speaker 2",
        "translation": "Y quiero una caja grande de tiza, también."
      },
      {
        "text": "Ik heb alleen kleine dozen. Wilt u er een?",
        "speaker": "Speaker 1",
        "translation": "Solo tengo cajas pequeñas. ¿Quiere una?"
      },
      {
        "text": "Nee, dank u.",
        "speaker": "Speaker 2",
        "translation": "No, gracias."
      },
      {
        "text": "Is dat alles?",
        "speaker": "Speaker 1",
        "translation": "¿Es eso todo?"
      },
      {
        "text": "Dat is alles, dank u.",
        "speaker": "Speaker 2",
        "translation": "Eso es todo, gracias."
      },
      {
        "text": "Wat wilt u nog meer?",
        "speaker": "Speaker 1",
        "translation": "¿Qué más quiere?"
      },
      {
        "text": "Ik wil mijn wisselgeld.",
        "speaker": "Speaker 2",
        "translation": "Quiero mi cambio."
      }
    ],
