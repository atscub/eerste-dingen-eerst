    {
      "id": 3,
      "title": "Les 3",
      "type": "dialogue",
      "dialogue": {
        "lines": [
          {
            "text": "Mijn jas en mijn paraplu alstublieft.",
            "speaker": "Koen",
            "translation": "Mi abrigo y mi paraguas, por favor."
          },
          {
            "text": "Hier is uw kaartje.",
            "speaker": "Bart",
            "translation": "Aquí está su ticket."
          },
          {
            "text": "Dank u, meneer.",
            "speaker": "Koen",
            "translation": "Gracias, señor."
          },
          {
            "text": "Nummer vijf.",
            "speaker": "Bart",
            "translation": "Número cinco."
          },
          {
            "text": "Hier is uw paraplu en uw jas.",
            "speaker": "Bart",
            "translation": "Aquí está su paraguas y su abrigo."
          },
          {
            "text": "Dit is niet mijn paraplu.",
            "speaker": "Koen",
            "translation": "Este no es mi paraguas."
          },
          {
            "text": "Sorry, meneer.",
            "speaker": "Bart",
            "translation": "Lo siento, señor."
          },
          {
            "text": "Is dit uw paraplu?",
            "speaker": "Bart",
            "translation": "¿Es este su paraguas?"
          },
          {
            "text": "Nee, dat is niet zo!",
            "speaker": "Koen",
            "translation": "¡No, no lo es!"
          },
          {
            "text": "Is dit zo?",
            "speaker": "Bart",
            "translation": "¿Es este?"
          },
          {
            "text": "Ja, dat is zo.",
            "speaker": "Koen",
            "translation": "Sí, así es."
          },
          {
            "text": "Dank u wel.",
            "speaker": "Koen",
            "translation": "Muchas gracias."
          }
        ],
